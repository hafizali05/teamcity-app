{
  "env": {
    "jasmine": true
  },
  "plugins": [
    "import"
  ],
  "extends": ["eslint:recommended"],
  "globals": {
    "window": true,
    "Headers":true,
    "chrome":true,
    "fetch": true,
    "angular": true,
    "DOMParser": true,
    "IS_DEV": true
  },
  "parser": "babel-eslint",
  "rules": {
    "comma-dangle": ["error", "always-multiline"],
    "semi": [2, "always"],
    "indent": ["error", "tab"],    
    "no-tabs": 0
    
  }  
}